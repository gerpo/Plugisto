{
  "name": "gerpo/plugisto",
  "description": "A plugin system for laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Adam Marciniak",
      "email": "adamarc@web.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Gerpo\\Plugisto\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plugisto\\Tests\\": "tests"
    }
  },
  "require": {
    "laravel/framework": "6.*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gerpo\\Plugisto\\PlugistoServiceProvider"
      ]
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "orchestra/testbench": "^4.0"
  }
}
